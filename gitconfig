[color]
  ui = always
[core]
  editor = mate -w
[alias]
  co = checkout
  ci = commit
  cm = commit -m
  cma = commit --amend
  co = checkout
  st = status -sb
  diffc = diff --cached
  wc = whatchanged
  pl = pull
  plre = pull --rebase
  ps = push
  out = log origin..HEAD
  l = log --graph --pretty=format:'%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[color "diff"]
  meta = blue bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "branch"]
  current = yellow reverse
  local = yellow bold
  remote = green bold
  plain = red bold
[color "status"]
  added = yellow
  changed = green bold
  untracked = blue bold
